<%= render 'tabs' %>
<h2>Новый набор тестирования</h2>
<%= error_messages_for @testkit %>
<%= form_for(@testkit, url: project_testkits_path) do |f| %>
  <%= render :partial => 'form', locals: { :f => f } %>
<% end %>

<% content_for :header_tags do %>
  <%= javascript_include_tag 'jquery.fancytree-all.min', 'show_testcase', :plugin => 'redmine_testkit' %>
  <%= stylesheet_link_tag 'ui.fancytree.min', :plugin => 'redmine_testkit' %>
<% end %>

<%= javascript_tag do %>
  $(function() { $(document).dblclick({buttons: false}, dblclickShowTestcase) })
<% end %>


